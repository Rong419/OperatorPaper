<beast version='2.0'
       namespace='beast.evolution.alignment:beast.core:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>

    <!-- This xml uses a yule tree to simulate the sequences  -->

    <data spec='Alignment' id="input_alignment" dataType="nucleotide">
        <sequence taxon="1">?</sequence>
        <sequence taxon="2">?</sequence>
        <sequence taxon="3">?</sequence>
        <sequence taxon="4">?</sequence>
        <sequence taxon="5">?</sequence>
        <sequence taxon="6">?</sequence>
        <sequence taxon="7">?</sequence>
        <sequence taxon="8">?</sequence>
        <sequence taxon="9">?</sequence>
        <sequence taxon="10">?</sequence>
        <sequence taxon="11">?</sequence>
        <sequence taxon="12">?</sequence>
        <sequence taxon="13">?</sequence>
        <sequence taxon="14">?</sequence>
        <sequence taxon="15">?</sequence>
        <sequence taxon="16">?</sequence>
        <sequence taxon="17">?</sequence>
        <sequence taxon="18">?</sequence>
        <sequence taxon="19">?</sequence>
        <sequence taxon="20">?</sequence>
    </data>

    <tree id='tree' spec='beast.util.TreeParser'
          newick="(((((((1:0.01281820188,2:0.01281820188):0.02019383929,(((3:0.02598583234,10:0.02598583234):0.0007356707821,(8:0.007932209411,20:0.007932209411):0.01878929371):0.003164995646,(6:0.01665937673,11:0.01665937673):0.01322712204):0.003125542404):0.002729130065,13:0.03574117124):0.01732564463,5:0.05306681587):0.04094855399,(((7:0.001116725847,14:0.001116725847):0.02500611192,12:0.02612283777):0.005340500774,19:0.03146333854):0.06255203132):0.07107754487,(4:0.1035935311,16:0.1035935311):0.06149938364):0.01365357705,((9:0.04797652003,17:0.04797652003):0.03152175407,(15:0.01188760174,18:0.01188760174):0.06761067235):0.09924821769):0;"
	      IsLabelledNewick="true"/>

    <run spec="beast.app.seqgen.SequenceSimulator" id="seqgen"
         data='@input_alignment' tree='@tree' sequencelength='LENGTH'
         outputFileName="20taxaAlignmentLength_LENGTH.xml">

    <siteModel spec='SiteModel' id="siteModel" gammaCategoryCount='1'>
      <substModel spec='HKY' id='hky'>
      <parameter name='kappa' id="hky.kappa" value="2.47512458064281"/>
      <frequencies id='freqs' spec='Frequencies' frequencies="0.219302210545184 0.22684972081053 0.300725958248762 0.253122110395524"/>
      </substModel>
    </siteModel>

    <branchRateModel id="UCRelaxedClock" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" tree="@tree">
      <parameter id="ucldMean" estimate="false" name="clock.rate">1.0</parameter>
      <parameter id="branchrates" name="rateCategories" spec="parameter.IntegerParameter" dimension="38">1</parameter>
      <distr id='lognormal' spec="beast.math.distributions.LogNormalDistributionModel" meanInRealSpace="true">
      <parameter name='M' id='M' value="1.0"/>
      <parameter name='S' id='ucld.stdev' value="0.180357879544435"/>
      </distr>
    </branchRateModel>

    </run>

</beast>
